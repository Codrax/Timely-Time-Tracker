program TTU;



{$R *.dres}

uses
  Vcl.Forms,
  TTUmu in 'TTUmu.pas' {Timely},
  Vcl.Themes,
  Vcl.Styles;

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Windows10');
  Application.CreateForm(TTimely, Timely);
  Application.Run;
end.
